<odoo>


    <record id="view_migration_analysis_line_form" model="ir.ui.view">
        <field name="model">migration.analysis.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="line_serie_ids">
                            <tree>
                                <field name="serie_id" />
                                <field name="owner_type" />
                                <field name="state" />
                            </tree>
                        </field>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
